
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/c000000.wifi'
	option country 'US'
	option legacy_rates '1'
	option mu_beamformer '0'
	option band '5g'
	option channel '36'
	option htmode 'HE80'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/soc/c000000.wifi+1'
	option country 'US'
	option band '2g'
	option channel '10'
	option htmode 'HT40'
	option legacy_rates '1'
	option mu_beamformer '0'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option encryption 'psk2'
	option key '12345678123'
	option ssid 'CL_5G'
	option network 'lan'

config wifi-iface
	option device 'radio1'
	option mode 'ap'
	option ssid 'CL'
	option encryption 'psk2'
	option key '12345678123'
	option disabled '1'
	option network 'lan'

